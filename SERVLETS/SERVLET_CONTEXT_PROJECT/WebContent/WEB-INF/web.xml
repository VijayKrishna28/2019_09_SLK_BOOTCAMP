<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">

<context-param>
<param-name>company</param-name>
<param-value>vfislk</param-value>
</context-param>


<context-param>
<param-name>address</param-name>
<param-value>GreenPark</param-value>
</context-param>


<servlet>
<servlet-name>ijk</servlet-name>
<servlet-class>com.slk.Servlet1</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>ijk</servlet-name>
<url-pattern>/Servlet1</url-pattern>
</servlet-mapping>



<servlet>
<servlet-name>efg</servlet-name>
<servlet-class>com.slk.Servlet2</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>efg</servlet-name>
<url-pattern>/Servlet2</url-pattern>
</servlet-mapping>







</web-app>


